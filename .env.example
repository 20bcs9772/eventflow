# EventFlow Environment Configuration
# Copy this file to .env and fill in your values

# API Configuration
API_URL=http://10.0.2.2:3000

# Firebase / Google Sign-In Configuration
# Get from: Firebase Console > Authentication > Sign-in method > Google > Web client ID
GOOGLE_WEB_CLIENT_ID=your_web_client_id_here

# Feature Flags
ENABLE_GOOGLE_SIGNIN=true
ENABLE_APPLE_SIGNIN=true
